// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}


model Alunos{
  id Int @id @default(autoincrement())
  firtName String
  email String
  age String
  grade Int[]
  create_at DateTime
  stutus Boolean
}

